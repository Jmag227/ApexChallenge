@model IEnumerable<ApexChallenge>

<h3>Badges</h3>

<tr>
    <th>ID</th>
    <th>Name</th>
    <th>Description</th>
    <th>"BadgeIcon"</th>
</tr>

@foreach (var data in Model)
{
    <tr>
        <td>@data.Id</td>
        <td>@data.Name</td>
        <td>@data.Description</td>
        <td>@.</td>

    </tr>
}