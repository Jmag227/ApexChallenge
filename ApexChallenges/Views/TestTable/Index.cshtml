@model IEnumerable<TestTable>

<h2>TestTable</h2>
<table class="table">
    
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
    </tr>

    @foreach (var data in Model)
    {
        <tr>
            <td>@data.Id</td>
            <td>@data.Name</td>
            <td>@data.Description</td>

        </tr>
     }
</table>
